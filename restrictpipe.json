      "saveToRealtime": true,
      "outputDataModelName": "mdmuser",
      "useBatchNotification": false,
      "saveToUnified": true,
      "cronExpressions": [
        "0 0 0 1 1 ? 2099"
      ],
      "overlapDeltaMinutes": 0,
      "sourceEntities": {
        "dataModels": [
          {
            "dataModelName": "mdmuser"
          }
        ],
        "stagings": [
          {
            "connectorName": "nlp",
            "stagingName": "usertable"
          },
          {
            "pipelineName": "product_main",
            "pipelineDescription": "Pipeline for the product table, that uses staging nlp_product",
            "outputDataModelName": "mdmproduct",
            "overlapDeltaMinutes": 120,
            "saveToCds": true,
            "saveToUnified": true,
            "saveToRealtime": true,
            "sendToSubscriptions": true,
            "useBatchNotification": false,
            "cronExpressions": [
              "0 0/5 * ? * MON-FRI *",
              "0 0/15 * ? * SAT,SUN *"
            ],
            "prepareScripts": [
              "product_prepare.sql"
            ],
            "processScript": "xptoproduct.csql",
            "sourceEntities": {
              "dataModels": [
                {
                  "dataModelName": "mdmproduct"
                }
              ],
              "stagings": [
                {
                  "connectorName": "xpto",
                  "stagingName": "product"
                }
              ]
            }
          },
          {
            "pipelineName": "purchaseorder_main",
            "pipelineDescription": "Pipeline for the product table again, for batch partial process scenario",
            "outputDataModelName": "mdmpurchaseorder",
            "overlapDeltaMinutes": 72,
            "saveToCds": true,
            "saveToUnified": true,
            "saveToRealtime": true,
            "sendToSubscriptions": true,
            "useBatchNotification": false,
            "cronExpressions": [
              "0 * * ? * *"
            ],
            "prepareScripts": [
              "purchaseorder_prepare.sql"
            ],
            "processScript": "xptopurchaseorder.csql",
            "sourceEntities": {
              "dataModels": [
                {
                  "dataModelName": "purchaseorders"
                }
              ],
              "stagings": [
                {
                  "connectorName": "xpto",
                  "stagingName": "purchaseorder"
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
